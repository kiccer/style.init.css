{
  "name": "style.init.css",
  "description": "Page style initialization, and make the style look more normal in some browsers.",
  "version": "1.2.0",
  "version_short": "1.2",
  "keywords": [
    "css",
    "sass",
    "front-end",
    "web",
    "browser",
    "compatibility"
  ],
  "main": "./dist/style.init.css",
  "scripts": {
    "dev": "set NODE_ENV=development&& webpack --progress --colors --watch --mode=development",
