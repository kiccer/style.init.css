    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "sugarss": "^2.0.0",
    "tar": "^4.4.8",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2"
